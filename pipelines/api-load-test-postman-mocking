# Pipeline for apig staging

trigger: none

pool:
  vmImage: ubuntu-latest

steps:
- template: /pipelines/api-configure-stages-template.yml  # Template reference
  parameters:
    tenant: ${{parameters.tenant}}
    target_types:
    - CONFIG
    - BUILD
    - DEV_INT
    - DEV_EXT
    - STAGE_INT
    - STAGE_EXT
    - PROD_INT
    - PROD_EXT