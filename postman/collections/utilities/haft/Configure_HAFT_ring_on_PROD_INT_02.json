		{
			"name": "Configure ring on node 02",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "1411581e-5aff-4526-949e-f574b133c044",
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"console.log(pm.request.body)\r",
							"console.log(pm.response.json())"
						],
						"type": "text/javascript"
					}
				}
			],
			"id": "0dc349b0-53ab-4385-8676-fb24f1d5cb4f",
			"protocolProfileBehavior": {
				"disableBodyPruning": true
			},
			"request": {
				"auth": {
					"type": "basic",
					"basic": [
						{
							"key": "username",
							"value": "{{haft_manager_user}}",
							"type": "string"
						},
						{
							"key": "password",
							"value": "{{haft_manager_password}}",
							"type": "string"
						}
					]
				},
				"method": "PUT",
				"header": [
					{
						"key": "Accept",
						"type": "text",
						"value": "application/json"
					},
					{
						"key": "Host",
						"value": "{{hostname}}"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"ring\": [\r\n        {\r\n            \"nodeName\": \"{{nodeName_01}}\",\r\n            \"host\": \"{{ip_grpc_01}}\",\r\n            \"port\": {{port_grpc_01}}\r\n        }\r\n    ]\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{protocol}}://{{ip}}:{{port}}/rest/apigateway/dataspace/ring",
					"protocol": "{{protocol}}",
					"host": [
						"{{ip}}"
					],
					"port": "{{port}}",
					"path": [
						"rest",
						"apigateway",
						"dataspace",
						"ring"
					]
				}
			},
			"response": []
		},
