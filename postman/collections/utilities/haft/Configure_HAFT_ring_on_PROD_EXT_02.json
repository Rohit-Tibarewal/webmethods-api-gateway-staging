					"raw": "{{protocol}}://{{ip}}:{{port}}/rest/apigateway/dataspace/ring",
					"protocol": "{{protocol}}",
					"host": [
						"{{ip}}"
					],
					"port": "{{port}}",
					"path": [
						"rest",
						"apigateway",
						"dataspace",
						"ring"
					]
				}
			},
			"response": []
