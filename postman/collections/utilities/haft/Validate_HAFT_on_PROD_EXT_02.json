	"item": [
		{
			"name": "Get dataspace status on node 02",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "d64f89d0-8bff-49cc-890e-1c44bcc5a5bb",
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"console.log(pm.response.json())"
						],
						"type": "text/javascript"
					}
				}
			],
			"id": "d16226d4-cc21-4dd6-bc28-797869958110",
			"protocolProfileBehavior": {
				"disableBodyPruning": true
			},
			"request": {
				"auth": {
					"type": "basic",
					"basic": [
						{
							"key": "username",
							"value": "{{haft_manager_user}}",
							"type": "string"
						},
						{
							"key": "password",
							"value": "{{haft_manager_password}}",
							"type": "string"
						}
					]
				},
				"method": "GET",
				"header": [
					{
						"key": "Accept",
						"type": "text",
						"value": "application/json"
					},
					{
						"key": "Host",
						"value": "{{hostname}}"
					}
				],
				"url": {
					"raw": "{{protocol}}://{{ip}}:{{port}}/rest/apigateway/dataspace/status",
					"protocol": "{{protocol}}",
					"host": [
						"{{ip}}"
					],
					"port": "{{port}}",
					"path": [
						"rest",
						"apigateway",
						"dataspace",
						"status"
					]
				}
			},
			"response": []
		}
	],
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					""
				],
				"id": "d484ed96-f3ec-47b5-8b06-64461e91af90"
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"let requests = pm.globals.has('requests') ?  JSON.parse(pm.globals.get('requests')) : [];",
					"request = {};",
					"request.method = pm.request.method;",
					"request.url = pm.request.url;",
					"request.body = pm.request.body;",
					"response = {};",
					"response.status = pm.response.status;",
					"response.body = pm.response.json();",
					"requests.push({\"request\": request, \"response\": response});",
					"pm.globals.set('requests', JSON.stringify(requests));"
				],
				"id": "9ad30733-c1b6-46d0-9f9f-dc597298ec31"
			}
		}
	]
