						}
					]
				},
				"method": "PUT",
				"header": [
					{
						"key": "Accept",
						"value": "application/json",
						"type": "text"
					},
					{
						"key": "Host",
						"value": "{{hostname}}"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"websocketUrls\": [],\r\n    \"httpsUrls\": [\r\n        \"https://playdevint.apigw-aw-eu.webmethods.io:443\",\r\n        \"https://lb-001:443\",\r\n        \"https://lb-002:443\",\r\n        \"https://lb-003:443\"\r\n    ],\r\n    \"httpUrls\": [\r\n        \"http://playdevint.apigw-aw-eu.webmethods.io:80\",\r\n        \"http://lb-001:80\",\r\n        \"http://lb-002:80\",\r\n        \"http://lb-003:80\"\r\n    ]\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{protocol}}://{{ip}}:{{port}}/rest/apigateway/configurations/loadBalancer",
					"protocol": "{{protocol}}",
					"host": [
						"{{ip}}"
					],
					"port": "{{port}}",
					"path": [
						"rest",
						"apigateway",
						"configurations",
						"loadBalancer"
					]
				}
			},
			"response": []
